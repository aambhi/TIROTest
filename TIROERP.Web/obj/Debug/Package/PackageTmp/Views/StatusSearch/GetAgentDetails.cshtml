    function isAlphaNumeric() {
        var charCode = event.keyCode;
        if ((charCode >= 48 && charCode <= 57) ||
              (charCode >= 65 && charCode <= 90) ||
              (charCode >= 97 && charCode <= 122) || charCode == 32)
            return true;

        return false;
    }

    $(document).ready(function () {
        $("#INDUSTRY").chosen({ width: '100%' });
        $("#DESIGNATION").chosen({ width: '100%' });
        $("#CURRENT_LOCATION").chosen({ width: '100%' });

        $("#CONTACT_REMARK").focusout(function () {
            $(".btn-default").focus();
        });

        var industry = '@Model.MULTIPLE_INDUSTRY_ID';
        var industryselectedValues = industry.split(',');
        for (a in industryselectedValues) {
            industryselectedValues[a] = parseInt(industryselectedValues[a], 10);
        }
        $("#INDUSTRY").val(industryselectedValues).trigger("chosen:updated").change();

        $("#SaveImage").change(function (e) {
            var file = e.target.files;
            var path = UploadAttachment(file, "Agent");
        });

        $("#SaveImageWithImg").change(function (e) {
            var file = e.target.files;
            var path = UploadAttachment(file, "Agent");
        });

        function UploadAttachment(file, uploadFrom) {
            var path = "";
            if (file.length > 0) {
                var isvalid = checkImagefile(file);
                if (isvalid) {
                    var data = new FormData();

                    // Add the uploaded image content to the form data collection
                    data.append("UploadedImage", file[0]);
                    data.append("UploadFrom", uploadFrom);

                    // Make Ajax request with the contentType = false, and procesDate = false
                    var ajaxRequest = $.ajax({
                        type: "POST",
                        url: '@Url.Action("UploadImageFile", "StatusSearch")',
                        contentType: false,
                        processData: false,
                        data: data
                    });

                    ajaxRequest.done(function (xhr, textStatus) {
                        // Do other operation
                        path = xhr;
                        if (uploadFrom == "Agent") {
                            $("#FILE_PATH").val(path);
                        }
                    });
                }
            }
            else {
                path = "";
                if (uploadFrom == "Agent") {
                    $("#FILE_PATH").val(path);
                }
            }
        }
    });
</script>