@model TIROERP.Core.Model.Advertisement
@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm("Delete", "Advertisement", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @id = "formControl", @class = "form-horizontal", enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()

    <div class="right_col" role="main">
        <div class="">
            <div class="page-title">
                <div class="title_left">
                    <h3>Advertisement</h3>
                </div>
            </div>
            <div class="clearfix"></div>
            <div class="row">
                <div class="col-md-12 col-sm-12 col-xs-12">
                    <div class="x_panel">
                        <div class="x_title">
                            <h2>Delete </h2>
                            <ul class="nav navbar-right panel_toolbox" style="min-width: 0px;">
                                <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a></li>
                            </ul>
                            <div class="clearfix"></div>
                        </div>
                        <div class="x_content">
                            <form class="form-inline">

                                <div class="form-group">
                                    <label for="middle-name" class="control-label col-md-3 col-sm-3">Paper Name <span class="required">*</span></label>
                                    <div class="col-md-6 col-sm-6" style="padding-top:10px; font-weight:bold;">
                                        @Html.DisplayFor(model => model.PAPER_NAME, new { @id = "PAPER_NAME", @class = "form-control", @autocomplete = "off", @maxlength = "50" })
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-md-6 col-sm-6 col-xs-12 col-md-offset-3">
                                        <button type="submit" class="btn btn-success">Submit</button>
                                        | @Html.ActionLink("Back to List", "Index")
                                    </div>
                                </div>
                                @Html.HiddenFor(model => model.ADV_ID)
                            </form>
                            <br />
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}
