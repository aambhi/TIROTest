@model TIROERP.Core.Model.City
@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm())
{
    <div class="right_col" role="main">
        <div class="">
            <div class="page-title">
                <div class="title_left">
                    <h3>City Master </h3>
                </div>
            </div>
            <div class="clearfix"></div>


            <div class="row">
                <div class="col-md-12 col-sm-12 col-xs-12">
                    <div class="x_panel">
                        <div class="x_title">
                            <h2>Delete </h2>
                            <ul class="nav navbar-right panel_toolbox" style="min-width: 0px;">
                                <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a></li>
                            </ul>
                            <div class="clearfix"></div>
                        </div>
                        <div class="x_content">
                            <form class="form-inline">

                                @*<div class="col-md-10">
                                    <label for="middle-name" class="control-label col-md-3 col-sm-3">City Code </label>
                                    <div class="col-md-6 col-sm-6">
                                        @Html.DisplayFor(model => model.CITY_CODE, new { @id = "CITY_CODE", @class = "form-control  col-md-7 col-xs-12", @autocomplete = "off", @maxlength = "50" })
                                    </div>
                                </div>*@

                                <div class="col-md-10" style="margin-top:10px;">
                                    <label for="middle-name" class="control-label col-md-3 col-sm-3">City Name <span class="required">*</span></label>
                                    <div class="col-md-6 col-sm-6">
                                        @Html.DisplayFor(model => model.CITY_NAME, new { @id = "CITY_NAME", @class = "form-control  col-md-7 col-xs-12", @autocomplete = "off", @maxlength = "50" })
                                    </div>
                                </div>
                                <div class="col-md-10" style="margin-top:10px;">
                                    <div class="col-md-6 col-sm-6 col-xs-12 col-md-offset-3">
                                        <button type="submit" class="btn btn-success">Delete</button>
                                        | @Html.ActionLink("Back to List", "Index")
                                    </div>
                                </div>
                                @Html.HiddenFor(model => model.CityId)
                            </form>
                            <br />
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}

